#include <stdio.h>
int main(){
    int n,sum=0,add=0;
    printf("enter number:");
    scanf("%d",&n);
    for (int i=1;i<=n;i++){
        if (i%2==0){
            sum=sum+i;
        }
        else if (i%2!=0){
            add=add+i;
        }
    }
    printf("%d",sum);
    printf("\n%d",add);


    return 0;
}
