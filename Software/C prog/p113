#include <stdio.h>
int main(){
    int n,sum=1,add=0;
    scanf("%d",&n);
    int temp=n;
    for (int i=n;n!=0;i++){
        int a=n%10;
        sum=1;
        for(int j=1;j<=a;j++){
        sum=sum*j;
        }
        add=add+sum;
        n=n/10;
    }
    if(temp==add){
    printf("strong");
    }
    else{
        printf("not strong");
    }

    return 0;
}
