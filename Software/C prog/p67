#include <stdio.h>
int main(){
    int n,sum=0;
    printf("enter no:");
    scanf("%d",&n);
    for (int i=1;i<n;i++){
        if(n%i==0){
            printf(" %d",i);
            sum=sum+i;
        }
        
    }
    printf("\n%d",sum);
    if (n==sum){
        printf("\nequal");
    }
    else if(n!=sum){
        printf("\nnot equal");
    }
    
    return 0;
}
