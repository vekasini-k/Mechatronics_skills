#include <stdio.h>

int main() {
    int n1,n2,sum=0;
    printf("Enter no: ");
    scanf("%d %d", &n1,&n2);
    int c=n1;
    for(int i=1;i<=n2;i++) {
     printf("%d",n1);
     if (i!=n2){
          printf("+");
     }
     sum=sum+n1;
     n1=n1*10+c;
    }

    printf("\n%d",sum);
    return 0;
}
