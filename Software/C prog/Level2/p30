#include <stdio.h>
int main(){
    int n,sum=0,rev=0;
    scanf("%d",&n);
    for (int i=1;i<=n;i++){
        int temp=i;
        while(temp!=0){
            int a=temp%10;
            rev=rev*10+a;
            temp=temp/10;
        }
        if (i==rev){
            sum=sum+rev;
        }
        rev=0;
    }
    printf("%d",sum);
    
    return 0;
}
