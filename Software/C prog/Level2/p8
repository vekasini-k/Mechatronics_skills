#include <stdio.h>
int main(){
    int n,c=1,sum=0;
    printf("enter no:");
    scanf("%d",&n);
    if (n==0){
        printf("%d",c);
    }
    else if(n!=0){
        for (int i=n;n!=0;i++){
            int a=n%10;
            sum++;
            n=n/10;
        }
        printf("%d",sum);
    }

    return 0;
}
