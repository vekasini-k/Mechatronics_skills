#include <stdio.h>
int main(){
    int n,isPrime=0;
    scanf("%d",&n);
    for(int i=2;i<=n;i++){
        isPrime=0;
        for(int j=2;j<i;j++){
            if(i%j==0){
                isPrime++;
                break;
            }
        }
        if(isPrime==0){
            for(int k=1;k<=i;k++){
            printf(" *");
        }
        printf("\n");
    }
}
    return 0;
}
