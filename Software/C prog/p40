#include <stdio.h>
int main(){
    int a,b,c;
    printf("enter 3 angle in degree");
    scanf("%d %d %d",&a,&b,&c);
    if (a+b+c>180){
        printf("invalid");
    }
    else if (a+b+c<90){
        printf("acute angle");
    }
    else if (a+b+c==90){
        printf("right angle");
    }
    else if (a+b+c>90){
        printf("obtuse angle");
    }

    return 0;
}
