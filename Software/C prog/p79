#include <stdio.h>
int main(){
    int n,c=0,sum=0,j;
    scanf("%d",&n);
    for(int i=n;c<5;i++){
        sum=0;
        i=j;
        while(j!=0){
            int a=j%10;
            sum=sum*10+a;
            j=j/10;
        }
        if(i==sum){
            printf("%d",i);
            c++;
        }
    }

    return 0;
}
