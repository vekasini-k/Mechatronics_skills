#include <stdio.h>
int main(){
    int n;
    printf("enter num");
    scanf("%d",&n);
    if (n%3==0){
        printf("\nmultiple of 3");
    }
    else{
        printf("\nnot multiple of 3");
    }
    if (n%6==0){
        printf("\nmultiple of 6");
    }
    else{
        printf("\nnot multiple of 6");
    }
    if (n%9==0){
        printf("\nmultiple of 9");
    }
    else{
        printf("\nnot multiple of 9");
    }


    return 0;
}
