#include <stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int n1,n2;
    scanf("%d %d",&n1,&n2);
    if(n1>=0 && n1<n && n2>=0 && n2<n){
        int t=arr[n1];
        arr[n1]=arr[n2];
        arr[n2]=t;
    }
    for(int i=0;i<n;i++){
        printf("%d",arr[i]);
    }

    return 0;
}
