#include <stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n],earr[n],oarr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }

    int e=0,o=0;
    for(int i=0;i<n;i++){
        if(arr[i]%2==0){
            earr[e]=arr[i];
            e++;
        }
        else{
            oarr[o]=arr[i];
            o++;
        }
    }
    for(int i=0;i<e;i++){
        printf(" %d",earr[i]);
    }
    
    for(int i=0;i<o;i++){
        printf(" %d",oarr[i]);
    }
    printf("\n");
    int t;
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            if(arr[i]>arr[j]){
                t=arr[i];
                arr[i]=arr[j];
                arr[j]=t;
            }
        }
    }
    for(int i=0;i<n;i++){
        printf(" %d",arr[i]);
    }

    return 0;
}
