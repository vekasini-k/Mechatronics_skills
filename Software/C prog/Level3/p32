#include <stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int k,c=0;
    scanf("%d",&k);
    for(int i=0;i<n;i++){
        if(arr[i]==k){
            c=1;
        }
        if(c==1){
            printf("%d",i+1);
            break;
        }
    }
    return 0;
}
