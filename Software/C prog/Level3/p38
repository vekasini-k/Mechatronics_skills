#include <stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int as=1;
    for(int j=0;j<n-1;j++){
        if(arr[j]>arr[j+1]){
            as=0;
            break;
        }
    }
    int ds=1;
    for(int j=0;j<n-1;j++){
        if(arr[j]<arr[j+1]){
            ds=0;
            break;
        }
    }
    if(as==1 || ds==1){
        printf("sorted");
    }
    else{
        printf("not sorted");
    }

    return 0;
}
