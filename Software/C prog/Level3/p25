#include <stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int k;
    scanf("%d",&k);
    for(int i=0;i<n;i+=k){
        printf("[");
        for(int j=i;j<i+k&&j<n;j++){
            printf("%d",arr[j]);

            if(j<i+k-1&&j<n-1){
                printf(",");
            }
        }
        printf("]");
    }
    
    return 0;
}
