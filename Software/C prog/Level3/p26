#include <stdio.h>
int main(){
    int n,num,pos;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    scanf("%d %d",&num,&pos);
    for(int i=n;i>=pos;i--){
        arr[i]=arr[i-1];
    }
    arr[pos-1]=num;
    n++;

    for(int a=0;a<n;a++){
        printf("%d",arr[a]);
    }

    return 0;
}
