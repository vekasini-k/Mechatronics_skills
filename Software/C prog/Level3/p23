#include <stdio.h>
#include <stdlib.h> 
int main() {
    int n;
    scanf("%d", &n);
    
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int minsum=__INT_MAX__;
    int num1,num2,sum=0;
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            sum=arr[i]+arr[j];
            if(abs(sum)<abs(minsum)){
                minsum=sum;
                num1=arr[i];
                num2=arr[j];
            }
        }
    }
    printf("%d,%d",num1,num2);

    return 0;
}
