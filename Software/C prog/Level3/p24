#include <stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int t;
    for(int j=0;j<n;j++){
        for(int k=0;k<n;k++){
            if(arr[j]<arr[k]){
                t=arr[j];
                arr[j]=arr[k];
                arr[k]=t;
            }
        }
    }
    for(int a=0;a<n;a++){
        printf(" %d",arr[a]);
    }
    double m;
    if(n%2==0){
        m=(arr[n/2]+arr[n/2-1])/2;
    }
    else if(n%2!=0){
        m=arr[n/2];
    }
    printf("\n%f",m);

    return 0;
}
