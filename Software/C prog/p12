#include <stdio.h>
int main(){
    int age;
    printf("enter your age");
    scanf("%d",&age);
    if (age>=1 && age<=12){
        printf("ticket price is 25 ");
    }
    else if (age>12 && age<=20){
        printf("ticket price is 50");
    }
    else if (age>20 && age<=60){
        printf("ticket price is 100");
    }
    else if (age>=61){
        printf("ticket price is 150");
    }
    else{
        printf("error");
    }
    
    return 0;
}
