#include <stdio.h>
int main()
{
    int roll,p,c,cs,tot,per;
    char name[50];

    printf("enter roll no:");
    scanf("%d",&roll);
    printf("\n enter name:");
    scanf(" %s",&name);
    printf("\n enter phy mark:");
    scanf("%d",&p);
    printf("\n enter chem mark:");
    scanf("%d",&c);
    printf("\n enter comp mark:");
    scanf("%d",&cs);
    
    tot=p+c+cs;
    printf("\n total mark: %d",tot);
    per=tot/3;
    printf("\n percentage: %d",per);
    if (per>=80){
        printf("\n first");
    }
    else if(per<80 && per>=70){
        printf("\n second");
    }
    else if(per<70 && per>=60){
        printf("\n third");
    }
    else if(per<60 && per>=50){
        printf("\n fourth");
    }
    else if(per<50 && per>40){
        printf("\n fifth");
    }
    else if(per<=40 && per>=0){
        printf("\n fail");
    }

    return 0;
}
