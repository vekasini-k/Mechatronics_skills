#include <stdio.h>
int main(){
    int s1,s2,s3;
    printf("enter 1st side");
    scanf("%d",&s1);
    printf("enter 2nd side");
    scanf("%d",&s2);
    printf("enter 3rd side");
    scanf("%d",&s3);
    if ((s1+s2>s3) && (s1+s3>s2) && (s2+s3>s1)){
        printf("triangle");

        if (s1==s2 && s2==s3){
        printf("\nequilateral triangle");
        }
        else if (s1==s2|| s2==s3 || s3==s1){
            printf("\nisosceles triangle");
        }
        else {
            printf("\nscalene");
        }
    }
    else {
        printf("not a triangle");
    }
    

    return 0;
}
