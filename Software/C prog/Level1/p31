#include <stdio.h>
int main(){
    int h;
    printf("enter height in cm:");
    scanf("%d",&h);
    if (h<150){
        printf("short");
    }
    else if (h>=150 && h<170){
        printf("medium");
    }
    else if (h>=170 && h<190){
        printf("tall");
    }
    else{
        printf("very tall");
    }
    return 0;
}
