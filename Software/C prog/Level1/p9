#include <stdio.h>
int main(){
    int age;
    printf("enter your age: ");
    scanf("%d",&age);

    if (age>=1 && age<=5){
        printf("eat mashed vegetables or soft fruits");
    }
    else if (age>5 && age<=18){
        printf("eat burgers or pasta");
    }
    else if (age>19 && age<51){
        printf("eat greens and spouts");
    }
    else if (age>=51){
        printf("eat soup and grilled fish");
    }
    else{
        printf("error");
    }



    return 0;
}
