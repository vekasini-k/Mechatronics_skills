#include <stdio.h>
int main(){
    printf("enter number of week");
    int num;
    scanf("%d",&num);
    if (num==1){
        printf("Sunday \n");
    }
    else if (num==2){
        printf("Monday \n");
    }
    else if (num==3){
        printf("Tuesday \n");
    }
    else if (num==4){
        printf("Wednessday \n");
    }
    else if (num==5){
        printf("Thursday \n");
    }
    else if (num==6){
        printf("Friday \n");
    }
    else if (num==7){
        printf("Saturday \n");
    }

    if (num>=2 && num<=7){
        printf("Weekday");
    }
    else if (num==1){
        printf("Holiday");
    }

    return 0;
}
