#include <stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int s=0,c=0;
    for(int j=0;j<n;j++){
        c=0;
        for(int k=j+1;k<n;k++){
            if(arr[j]==arr[k]){
                c++;
            }
        }
        if(c!=0){
            s=s+arr[j];
            printf(" %d",arr[j]);
        }
    }
    printf("\n%d",s);

    return 0;
}
