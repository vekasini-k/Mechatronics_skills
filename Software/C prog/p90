#include <stdio.h>
int main(){
    int n,a,rn=0;
    printf("enter no:");
    scanf("%d",&n);
    int temp=n;
    while (n>0){
        a=n%10;
        rn=rn*10+a;
        n=n/10;
    }
    
    if (temp==rn){
        printf("palindrome");
    }
    else{
        printf("not palindrome");
    }

    return 0;
}
