#include <stdio.h>
int main(){
    int n,max,min;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    max=min=arr[0];
    for(int j=0;j<n;j++){
        if(max<arr[j]){
            max=arr[j];
        }
        if(min>arr[j]){
            min=arr[j];
        }
    }
    printf("%d",max);
    printf("\n%d",min);

    return 0;
}
