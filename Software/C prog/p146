#include <stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n],earr[n],oarr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }

    int e=0,o=0;
    for(int i=0;i<n;i++){
        if(arr[i]%2==0){
            earr[e]=arr[i];
            e++;
        }
        else{
            oarr[o]=arr[i];
            o++;
        }
    }
    for(int i=0;i<e;i++){
        printf(" %d",earr[i]);
    }
    printf("\n");
    for(int i=0;i<o;i++){
        printf(" %d",oarr[i]);
    }

    return 0;
}
