#include <stdio.h>
int main(){
    int num1,num2,num,div1,div2;
    printf("enter two nos.:");
    scanf("%d %d",&num1,&num2);
    num=num1*num2;
    if (num%2==0){
        div1=num/2;
        printf("the ans is: %d",div1);
    }
    else{
        div2=num/3;
        printf("the ans is: %d",div2);
    }
    
    return 0;
}
