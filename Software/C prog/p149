#include <stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int k,s;
    scanf("%d",&k);
    int c=0;
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            s=arr[i]+arr[j];
            if(s==k){
                printf("%d %d",i,j);
                c=1;
                break;
            }
        }
        }

    return 0;
}
